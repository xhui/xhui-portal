<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	<navigation-rule>
		<navigation-case>
			<from-outcome>globalNav_home</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>globalNav_life</from-outcome>
			<to-view-id>/life.xhtml</to-view-id>
			<redirect>
				<redirect-param></redirect-param>
			</redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>globalNav_career</from-outcome>
			<to-view-id>/career.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>globalNav_contact</from-outcome>
			<to-view-id>/contact.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>